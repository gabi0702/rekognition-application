			Valid_Past_Date1 as "Valid Past Date"
			{
				type =  form
				form = Appointments
				record event = on add or edit

				on load
				{
					actions 
					{
						custom deluge script
						(
											if(input.turnDate != null)
										{
											DateOfTurn = input.turnDate;
											if(DateOfTurn < zoho.currentdate)
											{
												alert "אי אפשר להזמין תור לתאריך שעבר";
												AllHour = {"9:00","9:30","10:00","10:30","11:00","11:30","12:00","12:30","14:00","14:30","15:00","15:30","16:00","16:30","17:00","17:30","18:00","18:30","19:00","19:30"};
												AvailableHour = Appointments[turnDate == today].turnHour.getAll();
												if(AllHour.size() == AvailableHour.size())
												{
													if(today.weekday() == 5)
													{
														input.turnDate = today.addDay(3);
													}
													else
													{
														input.turnDate = today.addDay(1);
													}
												}
												else
												{
													input.turnDate = today;
												}
											}
										}
						)
					}
				}

			}
