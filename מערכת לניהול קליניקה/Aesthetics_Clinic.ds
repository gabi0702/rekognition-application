			Calculate_Total_Cost1 as "Calculate Total Cost"
			{
				type =  form
				form = AddClientsTreatments
				record event = on add or edit

				on user input of treatementOnTheClient.seringesAmount
				{
					actions 
					{
						custom deluge script
						(
											//Calculation of the total cost without taking into account the discount
										input.totalPriceForTreatements = 0;
										totalPrice = 0;
										for each  rec in treatementOnTheClient
										{
											rec.treatmentPrice=ifnull(rec.seringesAmount,0) * ifnull(rec.priceForTreat,0);
