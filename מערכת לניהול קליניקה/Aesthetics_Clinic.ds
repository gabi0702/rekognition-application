						(
											// Retrieving the current visit Reason from the DB
										current_visit_reason = Appointments[ID == input.ID].visitReason;
										//Prohibited checking between statuses
										if(current_visit_reason == "ייעוץ" && input.visitReason == "ביקורת")
										{
											input.visitReason = "ייעוץ";
											alert "לא ניתן לעבור ישירות לביקורת מייעוץ בלי לעבור טיפול";
										}
