											if(isBlank(input.treatementOnTheClient) || isBlank(input.makeSpecialPrice) || isBlank(input.totalPriceForTreatements) || isBlank(input.invoiceNumber))
											{
												alert "יש צורך למלא את פרטי הטיפול בלקוח ונתונים כספיים";
												cancel submit;
											}
										}
										//If the reason for the visit is a review
										if(input.visitReason == "טיפול")
										{
											if(isBlank(input.didInjectionInThePast) || isBlank(input.takemedicationRegularly) || isBlank(input.sufferingChronicDiseases) || isBlank(input.sufferFromAllergies) || isBlank(input.isPregnant) || isBlank(input.isBreastfeeding) || isBlank(input.clientSignature) || isBlank(input.treatementOnTheClient) || isBlank(input.makeSpecialPrice) || isBlank(input.totalPriceForTreatements))
											{
												alert "יש צורך למלא את השאלון בריאות, פרטי הטיפול בלקוח ונתונים כספיים";
												cancel submit;
											}
										}
										//If there is a discount, you need to make sure that you choose a discount type
										if(input.makeSpecialPrice == "כן" && isBlank(input.discountType))
										{
											alert "במידה ויש הנחה, צריך לסמן מהו סוג ההנחה";
											cancel submit;
										}
										//If the type of discount is shekels, the "discount in shekels" field should be made a mandatory field
										if(input.makeSpecialPrice == "כן" && input.discountType == "הנחה בשקלים" && isBlank(input.shekelDiscount))
										{
											alert "יש לבחור את כמות ההנחה בשקלים";
											cancel submit;
										}
										//If the discount type is a percentage, the "percentage discount" field should be made a mandatory field
										if(input.makeSpecialPrice == "כן" && input.discountType == "הנחה באחוזים" && isBlank(input.discountPercentage))
