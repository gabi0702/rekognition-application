			Valid_Quantity_In_the_Sto as "Valid Quantity In the Stock"
			{
				type =  form
				form = AddClientsTreatments
				record event = on add or edit

				on validate
				{
					actions 
					{
						custom deluge script
						(
											// Only a treatment can update the inventory level
										if(input.visitReason == "טיפול")
										{
											//Loop on all material sold
											for each  rec in treatementOnTheClient
											{
												//A variable that keeps the quantity value from material X in stock
												getStockDetails = Inventory[materialName == rec.materialNameUsed.materialName && stockDate == today];
												//First we will check that the material is in stock
												if(getStockDetails.count() != 0)
												{
													if(getStockDetails.stockLevel == 0)
													{
														alert "נגמר המלאי עבור חומר " + rec.materialNameUsed.materialName;
														cancel submit;
													}
													//If the quantity of stock is equal to the quantity sold, the sale is approved and a purchase order must be executed
													if(getStockDetails.stockLevel == rec.seringesAmount)
													{
														getStockDetails.stockLevel=getStockDetails.stockLevel - rec.seringesAmount;
														sendmail
														[
															from :zoho.loginuserid
															to :thisapp.permissions.loginUserEmailid()
															subject :"תזכורת - נגמר הכמות במלאי עבור החומר " + rec.materialNameUsed.materialName
															message :"לידיעתך, נגמר הכמות במלאי עבור החומר " + rec.materialNameUsed.materialName + ".<br/> יש לבצע הזמנה בקרוב."
														]
													}
													//If the quantity of stock is lower than the quantity sold cancel the sale
													if(getStockDetails.stockLevel < rec.seringesAmount && getStockDetails.stockLevel != 0)
													{
														alert "יש להקליד כמות קטנה מ- " + getStockDetails.stockLevel + " עבור החומר " + rec.materialNameUsed.materialName;
														cancel submit;
													}
													//If the stock is sufficient, the sale takes place as usual
													if(getStockDetails.stockLevel > rec.seringesAmount)
													{
														getStockDetails.stockLevel=getStockDetails.stockLevel - rec.seringesAmount;
													}
												}
											}
										}
						)
					}
				}

			}
