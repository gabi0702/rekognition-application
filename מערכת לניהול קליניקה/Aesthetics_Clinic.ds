			Reminder_about_customer_q as "Reminder about customer queues"
			{
				type =  schedule
					status = inactive
				start = "2022-08-20 21:00:00"
				frequency = weekly
				time zone = "Asia/Jerusalem"
				on start
				{
					actions 
					{
					on load
					(
						//The days to remember
										Sunday = today.addDay(1);
										Monday = today.addDay(2);
										Tuesday = today.addDay(3);
										Wednesday = today.addDay(4);
										Thursday = today.addDay(5);
										//The list of customers with this week's order
										AppointmentsOfCustomer = Appointments[turnDate == Sunday || turnDate == Monday || turnDate == Tuesday || turnDate == Wednesday || turnDate == Thursday];
										AllCustomer = AddCustomer[ID != 0];
										//Loop
										for each  customer in AllCustomer
										{
											for each  Appointment in AppointmentsOfCustomer
											{
												//Extracting the email address of the suppliers for the purpose of the reminder
												if(customer.customerID == Appointment.customerNameForAppointment)
												{
													HourOfTurn = Appointment.turnHour;
													DayOfTurnNumber = Appointment.turnDate.weekday();
													//days_of_the_week = {1:"ראשון",2:"שני",3:"שלישי",4:"רביעי",5:"חמישי"};
													days_of_the_week = {"יום ראשון","יום שני","יום שלישי","יום רביעי","יום חמישי"};
													DayOfTurn = days_of_the_week.get(DayOfTurnNumber - 1);
													sendmail
													[
														from :zoho.adminuserid
														to :customer.EmailCustomer
														subject :"תזכורת - תור"
														message :"השבוע יש לך בתור במרפאה ביום " + DayOfTurn + ", בשעה " + HourOfTurn
													]
												}
											}
										}
					)
					}
				}
			}
			Update_Stock_Daily as "Update Stock Daily"
			{
				type =  schedule
					status = inactive
				start = "2022-08-23 08:00:00"
				frequency = daily
				time zone = "Asia/Jerusalem"
				on start
				{
					actions 
					{
					on load
					(
						// A loop on all the materials the clinic has
										for each  material in AddMaterial[ID != 0]
										{
											yesterday = today.addDay(-1);
											inventory_line_yesterday = Inventory[materialName == material.materialName && stockDate == yesterday];
											//If there were inventory records yesterday, we will copy the records to today
											if(inventory_line_yesterday.count() != 0)
											{
												record_for_today = insert into Inventory
												[
													materialName=material.materialName
													stockLevel=inventory_line_yesterday.stockLevel
													stockDate=today
													Added_User=zoho.loginuser
												];
											}
											//There were no inventory records yesterday, we will create the record today with 0 inventory
											else
											{
												record_for_today = insert into Inventory
												[
													materialName=material.materialName
													stockLevel=0
													stockDate=today
													Added_User=zoho.loginuser
												];
											}
										}
					)
					}
				}
			}
			Appointment_Reminder_to_c as "Appointment Reminder to customers"
			{
				type =  schedule
					status = inactive
				start = "2022-09-14 09:00:00"
				frequency = daily
				time zone = "Asia/Jerusalem"
				on start
				{
					actions 
					{
					on load
					(
						tomorrow = today.addDay(1);
										//The list of customers with tomorrow appointment
										AppointmentsOfCustomer = Appointments[turnDate == tomorrow];
										AllCustomer = AddCustomer[ID != 0];
										//Loop
										for each  customer in AllCustomer
										{
											for each  Appointment in AppointmentsOfCustomer
											{
												//Extracting the email address of the suppliers for the purpose of the reminder
												if(customer.ID == Appointment.customerNameForAppointment)
												{
													turnDateFormat = tomorrow.toString("dd/MM/yy");
													HourOfTurn = Appointment.turnHour;
													sendmail
													[
														from :zoho.adminuserid
														to :customer.EmailCustomer
														subject :"תזכורת - תור למחר: " + turnDateFormat + " בשעה: " + HourOfTurn
														message :("שלום " + Appointment.customerNameForAppointment.customerName) + "<br>" + "זוהי תזכורת לקראת התור שלך במרפאה למחר בתאריך  " + turnDateFormat + " בשעה " + HourOfTurn + " ל" + Appointment.visitReason + "." + "<br>" + "לביטול או שינוי התור יש להתקשר למרפאה."
														content type :HTML
													]
												}
											}
										}
					)
					}
				}
			}
