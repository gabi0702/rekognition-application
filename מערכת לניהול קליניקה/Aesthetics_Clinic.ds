			Reminder_to_suppliers as "Reminder to suppliers"
			{
				type =  schedule
					status = inactive
				start = "2022-08-22 07:00:00"
				frequency = daily
				time zone = "Asia/Jerusalem"
				on start
				{
					actions 
					{
					on load
					(
						//3 days before the delivery date, a reminder will be sent to the supplier that his delivery date is approaching
										Three_more_days = today.addDay(3);
										SupplierList = AddPurchasesOrder[orderStatus == "נשלח" && deliveryDate == Three_more_days];
										AllSupplier = AddSupplier[ID != 0];
										//Loop over all providers in the list
										for each  supplier in AllSupplier
										{
											for each  reminderToSuppier in SupplierList
											{
												//Extracting the email address of the suppliers for the purpose of the reminder
												if(supplier.ID == reminderToSuppier.supplierNameForCommand)
