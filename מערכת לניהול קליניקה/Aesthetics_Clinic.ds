			Limit_number_queues_per_d as "Limit number queues per day per customer"
			{
				type =  form
				form = Appointments
				record event = on add or edit

				on validate
				{
					actions 
					{
						custom deluge script
						(
											// Saving the customer as a variable
										Customer_Name = input.customerNameForAppointment;
										//The client's appointments from that day
										Date_Of_Turn = input.turnDate;
										Turn_Of_Customer = Appointments[customerNameForAppointment == Customer_Name && turnDate == Date_Of_Turn].visitReason.getAll();
										// If the length of the list is different, it means that there are already 2 queues on the same day
										if(Turn_Of_Customer.size() == 2)
										{
											alert "לקוח לא יכול לקבוע יותר מ-2 תורים ביום";
											cancel submit;
										}
						)
					}
					actions 
					{
						custom deluge script
						(
											// Saving the customer as a variable
										Customer_Name = input.customerNameForAppointment;
										// The date of the client's appointment
										Date_Of_Turn = input.turnDate;
										// The reason for the customer's visit
										visitReason_Of_Customer = input.visitReason;
										visitReason_Of_Customer_in_This_day = Appointments[customerNameForAppointment == Customer_Name && turnDate == Date_Of_Turn && visitReason == visitReason_Of_Customer].visitReason.getAll();
										// Checking that the reason for the visit does not already exist
										for each  reason in visitReason_Of_Customer_in_This_day
										{
											if(reason == visitReason_Of_Customer)
											{
												alert "לקוח לא יכול לקבוע תור לאותה סיבה באותו יום";
												cancel submit;
											}
										}
						)
					}
				}

